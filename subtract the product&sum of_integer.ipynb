    "def subtract_prodandsum(num):\n",
    "    prod=1\n",
    "    s=0\n",
    "    while num>0:\n",
    "        d=num%10\n",
    "        prod*=d\n",
    "        s+=d\n",
    "        num//=10\n",
    "    return prod-s\n",
    "num=234\n",
    "print(subtract_prodandsum(num))\n",
    "    \n",
